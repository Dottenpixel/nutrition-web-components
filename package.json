{
  "name": "nutrition-web-components",
  "description": "Nutrition Web Components - NOVA and Nutri-score badges",
  "private": true,
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@stencil/core": "0.17.0",
    "@stencil/sass": "0.1.1",
    "@storybook/addon-knobs": "^4.1.7",
    "@storybook/html": "^4.1.7",
    "@wdio/browserstack-service": "^5.3.4",
    "@wdio/cli": "^5.3.5",
    "@wdio/dot-reporter": "^5.2.3",
    "@wdio/local-runner": "^5.3.5",
    "@wdio/mocha-framework": "^5.3.2",
    "@wdio/spec-reporter": "^5.2.3",
    "@wdio/sync": "^5.3.2",
    "babel-loader": "^8.0.5",
    "browserstack-local": "^1.3.7",
    "http-server": "^0.11.1",
    "lerna": "3.10.6",
    "npm-run-all": "^4.1.5",
    "webdriverio": "^5.3.5"
  },
  "scripts": {
    "DEV": "",
    "nutri-score.dev": "cd packages/nutri-score && stencil build --dev --watch --serve",
    "nova.dev": "cd packages/nova && stencil build --dev --watch --serve",
    "BUILD": "",
    "nutri-score.build": "cd packages/nutri-score && stencil build --build",
    "nutri-score.build:prod": "cd packages/nutri-score && stencil build --build --prod",
    "nova.build": "cd packages/nova && stencil build --build",
    "nova.build:prod": "cd packages/nova && stencil build --build --prod",
    "TEST-LOCAL": "",
    "nova.test.e2e.local": "run-p -r badges.e2e.serve nova.test.e2e.wdio:local",
    "nova.test.e2e.wdio:local": "npx wdio test-conf/wdio.conf.local.nova.js",
    "nutri-score.test.e2e.local": "run-p -r badges.e2e.serve nutri-score.test.e2e.wdio:local",
    "nutri-score.test.e2e.wdio:local": "npx wdio test-conf/wdio.conf.local.nutri-score.js",
    "TEST-REMOTE": "",
    "prebadges.test.e2e.remote": "run-s nova.build:prod nutri-score.build:prod",
    "badges.test.e2e.remote": "run-p -r badges.e2e.serve badges.test.e2e.wdio:browserstack",
    "badges.test.e2e.wdio:browserstack": "npx wdio test-conf/wdio.conf.js",
    "SERVE": "",
    "badges.e2e.serve": "http-server packages -s",
    "DOC": "",
    "storybook": "start-storybook -p 6006 -s packages/nova/www/build,packages/nutri-score/www/build",
    "build-storybook": "build-storybook -s packages/nova/www/build,packages/nutri-score/www/build"
  },
  "dependencies": {},
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vogloblinsky/nutrition-web-components.git"
  },
  "author": "Vincent Ogloblinsky",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vogloblinsky/nutrition-web-components/issues"
  },
  "homepage": "https://github.com/vogloblinsky/nutrition-web-components#readme"
}